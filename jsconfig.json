{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/contexts/*": ["src/contexts/*"],
      "@/layout/*": ["src/layout/*"],
      "@/hooks/*": ["src/utils/hooks/*"],
      "@/config/*": ["src/utils/config/*"],
      "@/constant/*": ["src/utils/constant/*"],
      "@/utils": ["src/utils"],
      "@/svg": ["asset/images/svg"],
      "@/svg/*": ["asset/images/svg/*"]
    }
  },
  "exclude": ["node_modules"]
}
