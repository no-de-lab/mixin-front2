{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/contexts/*": ["src/contexts/*"],
      "@/layout/*": ["src/layout/*"],
      "@/utils": ["src/utils"],
      "@/utils/*": ["src/utils/*"],
      "@/modules/*": ["src/modules/*"],
      "@/svg": ["asset/images/svg"],
      "@/svg/*": ["asset/images/svg/*"]
    },
    "experimentalDecorators": true
  },
  "exclude": ["node_modules"]
}
